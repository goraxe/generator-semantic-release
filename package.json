{
  "name": "@goraxe/generator-semantic-release",
  "version": "1.0.6",
  "description": "A simple semantic release composable generator",
  "homepage": "github.com/goraxe/generator-semantic-release",
  "type": "module",
  "author": {
    "name": "goraxe",
    "email": "goraxe@gmail.com",
    "url": ""
  },
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "generators"
  ],
  "main": "generators/index.js",
  "keywords": [
    "generator yeoman semantic-release yo yeoman-generator",
    "yeoman-generator"
  ],
  "devDependencies": {
    "@babel/core": "^7.23.6",
    "@babel/preset-env": "^7.23.6",
    "@semantic-release/changelog": "^6.0.0",
    "@semantic-release/git": "^10.0.0",
    "babel-jest": "^29.7.0",
    "coveralls": "^3.0.7",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-config-xo": "^0.29.1",
    "eslint-plugin-prettier": "^5.0.1",
    "fs-extra": "^11.0.0",
    "husky": "^9.0.0",
    "jest": "^29.0.0",
    "lint-staged": "^15.0.0",
    "prettier": "^3.0.0",
    "semantic-release": "^24.0.0",
    "yeoman-assert": "^3.1.0",
    "yeoman-test": "^8.0.0"
  },
  "engines": {
    "node": ">= 10.0.0"
  },
  "dependencies": {
    "chalk": "^5.0.0",
    "yeoman-generator": "^4.5.0",
    "yosay": "^3.0.0"
  },
  "jest": {
    "testEnvironment": "node",
    "collectCoverage": true
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix"
    ],
    "*.json": [
      "prettier --write"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "eslintConfig": {
    "extends": [
      "xo",
      "prettier"
    ],
    "env": {
      "jest": true,
      "node": true
    },
    "rules": {
      "prettier/prettier": "error"
    },
    "plugins": [
      "prettier"
    ]
  },
  "scripts": {
    "pretest": "eslint .",
    "test": "jest"
  },
  "repository": "goraxe/generator-semantic-release",
  "license": "MIT"
}
